    },
    {
        "text_box": [
            79,
            285,
            122,
            369
        ],
        "text": "YO! FATHER!!",
        "panel_id": "1",
        "type": "dialogue"
    },
    {
        "text_box": [
            240,
            41,
            319,
            135
        ],
        "text": "WE'RE ALL STUNNED AT THE TURN-AROUND!!",
