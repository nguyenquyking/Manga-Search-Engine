    {
        "text_box": [
            816,
            68,
            862,
            152
        ],
        "text": "My name is Stelly. I'm 8 years old.",
        "panel_id": "5",
        "type": "dialogue"
    },
    {
        "text_box": [
            798,
            172,
            873,
            346
        ],
        "text": "SHEEN!!!\nPleased to meet you, big brother!",
        "panel_id": "5",
        "type": "dialogue"
    },
    {
        "text_box": [
            934,
            64,
            981,
            145
        ],
        "text": "I've heard a lot about you.",
        "panel_id": "5",
        "type": "dialogue"
