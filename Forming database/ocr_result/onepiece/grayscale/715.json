    },
    {
        "text_box": [
            376,
            638,
            444,
            714
        ],
        "text": "RIGHT\nBEHIND\nYOU!!",
        "panel_id": "2",
        "type": "dialogue"
    },
    {
        "text_box": [
            767,
            67,
            880,
            182
        ],
        "text": "AND THE\nOPPORTUNITY\nTA AVENGE\nYOU IS ON\nTHIS ISLAND,",
        "panel_id": "3",
        "type": "narration"
    },
    {
        "text_box": [
            772,
            224,
            887,
            343
        ],
        "text": "WELL DONE,\nPEDRO...\nYOU DIED\nFER A NOBLE\nCAUSE!!",
