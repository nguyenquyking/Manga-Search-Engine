    },
    {
        "text_box": [
            397,
            65,
            503,
            197
        ],
        "text": "LUFFY'S CREW\nSAID IT WAS HIS\nSACRIFICE THAT\nENABLED THEM\nTO GET HERE...",
        "panel_id": "2",
        "type": "narration"
    },
    {
        "text_box": [
            401,
            229,
            484,
            335
        ],
        "text": "YES... IT\nHAPPENED IN\nBIG MOM'S\nTERRITORY!",
        "panel_id": "2",
        "type": "narration"
    },
    {
        "text_box": [
            384,
            384,
            480,
            467
        ],
        "text": "WHAT'S\nTHAT?!\nPEDRO'S\nDEAD?!",
        "panel_id": "2",
