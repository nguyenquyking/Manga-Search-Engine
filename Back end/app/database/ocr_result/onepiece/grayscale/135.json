[
    {
        "text_box": [
            204,
            167,
            296,
            319
        ],
        "text": "AHAHA\nHAHAHAHA\nHAHA!!",
        "panel_id": "1",
        "type": "dialogue"
    },
    {
        "text_box": [
            145,
            866,
            287,
            1060
        ],
        "text": "MODEL:\n\"NIKA\"!!",
        "panel_id": "1",
        "type": "narration"
    },
    {
        "text_box": [
            80,
            1295,
            201,
            1399
        ],
        "text": "THE HALL\nOF POWER\nPANGAEA\nCASTLE",
        "panel_id": "2",
        "type": "background"
    },
    {
        "text_box": [
            346,
            1278,
            443,
            1436
        ],
        "text": "ARE WE\nSURE OUR\nPRIORITIES ARE\nSTRAIGHT?",
        "panel_id": "2",
        "type": "dialogue"
    },
    {
        "text_box": [
            625,
            1254,
            727,
            1394
        ],
        "text": "...TO\nACQUIRE THE\nGOMU-GOMU\nFRUIT...",
        "panel_id": "2",
        "type": "dialogue"
    },
    {
        "text_box": [
            70,
            1993,
            148,
            2103
        ],
        "text": "THE\nHOLY LAND,\nMARY GEOISE",
        "panel_id": "3",
        "type": "background"
    },
    {
        "text_box": [
            324,
            1920,
            435,
            2061
        ],
        "text": "WE SACRIFICED\nONE OF OUR\nBEST AGENTS\nAND ENRAGED\nKAIDO.",
        "panel_id": "3",
        "type": "narration"
    },
    {
        "text_box": [
            640,
            1537,
            735,
            1724
        ],
        "text": "NO MATTER THE\nERA, THE WORLD\nGOVERNMENT HAS\nALWAYS FAILED...",
        "panel_id": "4",
        "type": "dialogue"
    },
    {
        "text_box": [
            611,
            1786,
            743,
            1907
        ],
        "text": "THAT WE\nSNUFF OUT\nSOMETHING\nTHIS\nPROBLEMATIC.",
        "panel_id": "4",
        "type": "dialogue"
    },
    {
        "text_box": [
            614,
            1955,
            721,
            2125
        ],
        "text": "THIS MAY BE\nBETTER THAN THE\nALTERNATIVE.\nIT'S IMPERATIVE...",
        "panel_id": "4",
        "type": "dialogue"
    },
    {
        "text_box": [
            856,
            242,
            998,
            411
        ],
        "text": "FAR AND WIDE...\nTHEY ARE THE\nEMBODIMENT OF\n\"THE WARRIOR OF\nLIBERATION,\"",
        "panel_id": "5",
        "type": "narration"
    },
    {
        "text_box": [
            812,
            440,
            1004,
            611
        ],
        "text": "THEY'RE\nSUPPOSEDLY\nONLY LIMITED\nBY THEIR\nIMAGINATION.\nPUTTING SMILES\nON FACES...",
        "panel_id": "5",
        "type": "narration"
    },
    {
        "text_box": [
            848,
            644,
            991,
            816
        ],
        "text": "IT'S USER'S\nBODY GAINS THE\nPROPERTIES\nOF RUBBER\nIN COMBAT,",
        "panel_id": "5",
        "type": "narration"
    },
    {
        "text_box": [
            875,
            1249,
            943,
            1370
        ],
        "text": "EVEN AFTER\n800 YEARS\nOF TRYING!!",
        "panel_id": "5",
        "type": "narration"
    },
    {
        "text_box": [
            1174,
            158,
            1350,
            378
        ],
        "text": "AWAKENING\nEXPONENTIALLY\nSTRENGTHENS THEIR\nRUBBERY BODY AND\nTHEY HAVE EVEN\nMORE FREEDOM",
        "panel_id": "6",
        "type": "narration"
    },
    {
        "text_box": [
            1432,
            85,
            1501,
            239
        ],
        "text": "IT IS\nALLEGEDLY...",
        "panel_id": "6",
        "type": "narration"
    },
    {
        "text_box": [
            1330,
            860,
            1437,
            1064
        ],
        "text": "\"SUN GOD\"\nNIKA!",
        "panel_id": "6",
        "type": "narration"
    },
    {
        "text_box": [
            1104,
            1280,
            1198,
            1394
        ],
        "text": "...IS\nREALLY A\nMYTHICAL\nZOAN...",
        "panel_id": "7",
        "type": "dialogue"
    },
    {
        "text_box": [
            1433,
            1280,
            1559,
            1383
        ],
        "text": "IT'S THE\nHUMAN-\nHUMAN\nFRUIT,",
        "panel_id": "7",
        "type": "dialogue"
    },
    {
        "text_box": [
            1079,
            1471,
            1225,
            1573
        ],
        "text": "AND\nTHIS ONE\nCARRIES\nTHE NAME\nOF A\n\"GOD.\"",
        "panel_id": "7",
        "type": "dialogue"
    },
    {
        "text_box": [
            1088,
            1619,
            1185,
            1773
        ],
        "text": "ALL ZOAN\nFRUITS HAVE A\nWILL OF THEIR\nOWN...",
        "panel_id": "7",
        "type": "dialogue"
    },
    {
        "text_box": [
            1108,
            1944,
            1216,
            2129
        ],
        "text": "IT'S ALMOST\nAS IF THE FRUIT\nITSELF IS\nEVADING US.",
        "panel_id": "7",
        "type": "dialogue"
    },
    {
        "text_box": [
            1340,
            1988,
            1414,
            2114
        ],
        "text": "WELL,\nTHAT'S NOT\nIMPOSSIBLE.",
        "panel_id": "7",
        "type": "dialogue"
    },
    {
        "text_box": [
            1429,
            1463,
            1535,
            1612
        ],
        "text": "AFTER\nALL, THE\nGOMU-GOMU\nFRUIT...",
        "panel_id": "8",
        "type": "dialogue"
    }
]